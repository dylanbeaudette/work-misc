@Article{Louche1987,
  Title                    = {Determination of {Angstrom's} turbidity coefficient from direct total solar irradiance measurements},
  Author                   = {Louche, A. and Maurel, M. and Simonnot, G. and Peri, G. and Iqbal, M.},
  Journal                  = {Solar Energy},
